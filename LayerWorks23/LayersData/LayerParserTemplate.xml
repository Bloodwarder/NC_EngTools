<?xml version="1.0" encoding="utf-8" ?>
<LayerParser>
	<Separator Value="_"/>
	<Classifiers>
		<Prefix Value="ИС"/>
		<PrimaryClassifier>
			<Chapter Value="ВО" Description="Водоотведение"/>
			<Chapter Value="ВС" Description="Водоснабжение"/>
			<Chapter Value="ГС" Description="Газоснабжение"/>
			<Chapter Value="ДК" Description="Дождевая канализация"/>
			<Chapter Value="ТС" Description="Теплоснабжение"/>
			<Chapter Value="ТТ" Description="Трубопроводный транспорт"/>
			<Chapter Value="СС" Description="Связь"/>
			<Chapter Value="ЭС" Description="Электроснабжение"/>
		</PrimaryClassifier>
		<AuxilaryData Name="ExternalProject" Brackets="\[\]"/>
		<AuxilartData Name="Diameter" Brackets="\{\}"/>
		<AuxilaryClassifier>
			<Classifier Value ="л" Description="Линейный"/>
			<Classifier Value ="п" Description="Площадной"/>
			<Classifier Value ="т" Description="Точечный"/>
		</AuxilaryClassifier>
		<SecondaryClassifiers/>
		<StatusClassifier>
			<Status Value="сущ"/>
			<Status Value="дем"/>
			<Status Value="проект"/>
			<Status Value="ндем"/>
			<Status Value="неутв"/>
			<Status Value="нреорг"/>
		</StatusClassifier>
		<BooleanPostfix Value="пер"/>
	</Classifiers>
	<SharedProperties>
		<Property Name="ConstantWidth">
			<StatusGroup>
				<StatusReference Value="сущ"/>
				<StatusReference Value="дем"/>
				<StatusReference Value="ндем"/>
				<DefaultValue>0.4</DefaultValue>
			</StatusGroup>
			<StatusGroup>
				<StatusReference Value="проект"/>
				<StatusReference Value="неутв"/>
				<StatusReference Value="нреорг"/>
				<DefaultValue>0.6</DefaultValue>
			</StatusGroup>
		</Property>
		<Property Name="Color">
			<StatusGroup>
				<StatusReference Value="сущ"/>
			</StatusGroup>
			<StatusGroup>
				<StatusReference Value="дем"/>
				<StatusReference Value="нреорг"/>
				<DefaultValue>
					<Red>107</Red>
					<Green>107</Green>
					<Blue>107</Blue>
				</DefaultValue>
			</StatusGroup>
			<StatusGroup>
				<StatusReference Value="проект"/>
			</StatusGroup>
			<StatusGroup>
				<StatusReference Value="ндем"/>
				<DefaultValue>
					<Red>192</Red>
					<Green>168</Green>
					<Blue>110</Blue>
				</DefaultValue>
			</StatusGroup>
			<StatusGroup>
				<StatusReference Value="неутв"/>
				<DefaultValue>
					<Red>0</Red>
					<Green>0</Green>
					<Blue>0</Blue>
				</DefaultValue>
			</StatusGroup>
		</Property>
		<Property Name="LinetypeName">
			<StatusGroup>
				<StatusReference Value="сущ"/>
				<StatusReference Value="дем"/>
				<StatusReference Value="ндем"/>
			</StatusGroup>
			<StatusGroup>
				<StatusReference Value="проект"/>
				<StatusReference Value="неутв"/>
				<StatusReference Value="нреорг"/>
			</StatusGroup>
		</Property>
		<Property Name="LinetypeScale">
			<StatusGroup>
				<StatusReference Value="сущ"/>
				<StatusReference Value="дем"/>
				<StatusReference Value="ндем"/>
			</StatusGroup>
			<StatusGroup>
				<StatusReference Value="проект"/>
				<StatusReference Value="неутв"/>
				<StatusReference Value="нреорг"/>
			</StatusGroup>
		</Property>
	</SharedProperties>
</LayerParser>
